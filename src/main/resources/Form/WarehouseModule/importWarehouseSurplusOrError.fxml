<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.WarehouseController.importWarehouseSurplusOrError">
   <children>
      <Label text="Nhập Lại Hàng" />
      <Label fx:id="id_TieuDe" text="Label" />
      <HBox spacing="20.0">
         <children>
            <Label text="Số Lượng" />
            <TextField fx:id="id_SoLuongNhap" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox spacing="20.0">
         <children>
            <Label text="Loại Nhập: " />
            <RadioButton fx:id="id_HAngDu" mnemonicParsing="false" text="Hàng Dư" />
            <RadioButton fx:id="id_HangLoi" mnemonicParsing="false" text="Hàng Lỗi" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <Button mnemonicParsing="false" onAction="#nhapKho" text="Nhập" />
      <TableView fx:id="tableViewPhieuXuatKho" prefHeight="300.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="col_Ma" prefWidth="116.0" text="Mã Phiếu Xuất Kho" />
          <TableColumn fx:id="id_SoLuongCongLai" prefWidth="135.20001220703125" text="Số Lượng Còn Lại" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
